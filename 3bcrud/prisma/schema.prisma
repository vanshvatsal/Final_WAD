generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model User {
    id        String   @id @default(uuid()) @map("_id")
    firstName String
    lastName  String
    age       Int
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt()
}
